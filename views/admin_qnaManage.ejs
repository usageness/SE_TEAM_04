    <script>
      
      $(document).ready(function(){
        
        var table = $('#table').DataTable({
          order:[[2,'desc']],
          ordering: true
        });
        // Event listener to the two range filtering inputs to redraw on input
        

        $('#min, #max').keyup( function() {
          table.draw();
        } );
        $('#min, #max').change(function() {
  table.draw();
});

$('#submit').click(
  function(){
    
    if (document.getElementById("startDate").value>=document.getElementById("endDate").value){
      alert("시작일은 종료일보다 크거나 같을수 없습니다.");
      return false;
    }
  }
);
        
      });
    </script>
  <script>
    

$.fn.dataTable.ext.search.push(
  function(settings, data, dataIndex) {
    var min_date = document.getElementById("min").value;
    var min = new Date(min_date);
    var max_date = document.getElementById("max").value;
    var max = new Date(max_date);

    var startDate = new Date(data[2]);
    var endDate = new Date(data[3]);
    //window.confirm(startDate);
    
    if (!min_date && !max_date) {
      return true;
    }
    if (!min_date && startDate <= max) {
      return true;
    }
    if (!max_date && endDate >= min) {
      return true;
    }
    if (startDate <= max && endDate >= min) {
      return true;
    }
    return false;
  },

  
);
