<div id="main-wrapper" style="overflow-y: auto">
  <div class="container">
    <h1>문의내역 현황</h1>
    <table border="0" cellspacing="5" cellpadding="5">
      <tbody><tr>
        <p>기간검색</p>
        <td><input type="date" id="min" name="min"> 부터 </td>
        <td><input type="date" id="max" name="max"> 까지 </td>
    </tr>
    <tr>
        <td></td>
        
    </tr>
</tbody>
      </table>
    <div class="my-3 p-3 pb-5 bg-body rounded shadow-sm">
      <table id="table" class="table" sortable="checked">
        <thead>
          <tr>
            <th scope="col">제목</th>
            <th scope="col">내용</th>
            <th scope="col">등록일시</th>
            <th scope="col">답변</th>
          </tr>
        </thead>
        <tbody>
          <% inquiryList.forEach(function(inquiry, i){ %>
            <tr>
              <td scope="row"><%= inquiry.title %></td>
              <td><%= inquiry.content %></td>
              <td><%= inquiry.date %></td>
              <% if(inquiry.answer) { %>
              <td><a href="/admin/qnaAns/<%= inquiry.id %>/end"><button>답변완료</button>✅</a></td>
              <% } else { %>
              <td><a href="/admin/qnaAns/<%= inquiry.id %>"><button>답변하기</button></a></td>
              <% } %>
            </tr>
           
            
            <% }) %>
        </tbody>
      </table>
    </div>
    
  </div>
</div>

  
