<main class="container">
  <form id="form" >
    <h1 class="mb-5 text-center">새 상품 등록하기</h1>
  
    <!-- Text input -->
    <div class="form-outline mb-4">
      <input type="text" id="form6Example3" class="form-control" name="name"/>
      <label class="form-label" for="form6Example3">보드게임 이름</label>
    </div>
  
    <!-- Text input -->
    <div class="form-outline mb-4">
      <input type="number" id="form6Example4" class="form-control" name="price"/>
      <label class="form-label" for="form6Example4">보드게임 가격</label>
    </div>

    <!-- Text input -->
    <div class="form-outline mb-4">
      <input type="number" id="formDeliverFee" class="form-control" name="deliveryFee"/>
      <label class="form-label" for="form6Example4">배송비 가격</label>
    </div>
  
    <!-- Email input -->
    <div class="form-outline mb-4">
      <input type="text" id="form6Example5" class="form-control" name="creator"/>
      <label class="form-label" for="form6Example5">제작사</label>
    </div>
    
    <div class="row mb-4">
      <!-- Number input -->
      <div class="col">
        <div class="form-outline mb-4">
          <input type="number" id="form6Example6" class="form-control" name="peopleMin"/>
          <label class="form-label" for="form6Example6">최소 인원</label>
        </div>
      </div>
      <!-- Number input -->
      <div class="col">
        <div class="form-outline mb-4">
          <input type="number" id="form6Example61" class="form-control" name="peopleMax"/>
          <label class="form-label" for="form6Example6">최대 인원</label>
        </div>
      </div>
    </div>
    <div class="form-outline mb-4">
      <input type="file" class="form-control" id="formMainImage" accept="image/*" name="mainImage" >
      <label class="form-label" for="formMainImage">메인 이미지</label>
    </div>

    <div class="form-outline mb-4">
      <input type="file" class="form-control" id="formSubImage" accept="image/*" name="subImage"  multiple >
      <label class="form-label" for="formSubImage">설명 이미지</label>
    </div>
    
    <div class="btn-group btn-group-toggle" id="divradio" data-toggle="buttons">
      <% for(var i=0; i<category.length; i++) {%>
       
        <label class="btn btn-outline-primary">
          <input type="radio" name="btnradio" id="btnradio<%= i %>"><%= category[i].name %></input>
        </label>
      <% } %>
    </div>
    <br>
    <label class="form-label" for="divradio">카테고리</label>

    <!-- Message input -->
    <div class="form-outline mb-4">
      <textarea class="form-control" id="form6Example7" rows="4" name="description"></textarea>
      <label class="form-label" for="form6Example7">상세 설명</label>
    </div>
  

    <!-- Submit button -->
    <button id="submitData" class="btn btn-primary btn-block mb-4">등록하기</button>
  </form>
</main>

    <script src="../assets/dist/js/bootstrap.bundle.min.js"></script>

    <script src="sidebars.js"></script>
    <script>
      async function readFileAsDataURL(file) {
        let result_base64 = await new Promise((resolve) => {
            let fileReader = new FileReader();
            fileReader.onload = (e) => resolve(fileReader.result);
            fileReader.readAsDataURL(file);
        });

        // console.log(result_base64); // aGV5IHRoZXJl...

        return result_base64;
      }
      $("#submitData").on("click", async (e) => {
        e.preventDefault();



        var formData = new FormData($("#form")[0])
        console.log(formData.get('btnradio'))
        console.log($('#divradio>.active').text().trim())


        if(formData.get('name') == "" ){
          alert('보드게임 이름은 필수 입력사항입니다.');
          return;
        }
        if(formData.get('price') == "" ){
          alert('보드게임 가격은 필수 입력사항입니다.');
          return;
        }
        if(formData.get('creator') == "" ){
          alert('제작사는 필수 입력사항입니다.');
          return;
        }
        if(formData.get('peopleMin') == "" ){
          alert('최소 인원은 필수 입력사항입니다.');
          return;
        }
        if(formData.get('peopleMax') == "" ){
          alert('최대 인원은 필수 입력사항입니다.');
          return;
        }
        if(formData.get('mainImage').name == "" ){
          alert('메인 이미지는 필수 입력사항입니다.');
          return;
        }
        if($('#divradio>.active').text().trim() == ""){
          alert('카테고리는 필수 입력사항입니다.');
          return;
        }
        if(formData.get('description') == "" ){
          alert('상세 설명은 필수 입력사항입니다.');
          return;
        }
        if(parseInt(formData.get('price'))%1000 != 0 ){
          alert('보드게임 가격은 1,000 단위로 입력해야 합니다.');
          return;
        }
        if(parseInt(formData.get('deliveryFee'))%1000 != 0 ){
          alert('배송비는 1,000 단위로 입력해야 합니다.');
          return;
        }
        if(parseInt(formData.get('deliveryFee')) < 1000 ){
          alert('배송비는 1,000 원보다 커야합니다.');
          return;
        }
        if(parseInt(formData.get('peopleMin')) > parseInt(formData.get('peopleMax')) ){
          alert('최소 인원이 최대 인원보다 적어야 합니다.');
          return;
        }
        if(parseInt(formData.get('peopleMin')) < 1 ){
          alert('최소인원은 1이상이어야 합니다.');
          return;
        }
        if(formData.get('description').length < 10 ){
          alert('상세 설명 10글자 이상 등록해야합니다.');
          return;
        }
        // if($("#formMainImage")[0].files.length != 0){
        //   formData.append("mainImage", $("#formMainImage")[0].files[0])
        //   console.log(formData.get("mainImage"))
        // }
        // for (var key of formData.entries()) {
        //   console.log(key[0] + ', ' + key[1]);
        // }
        formData.delete('mainImage')
        formData.append('mainImageName', $("#formMainImage")[0].files[0].name);
        formData.delete('subImage')

        if($("#formSubImage")[0].files.length != 0){
          var subImageFiles = []

          for(let i = 0 ; i < $("#formSubImage")[0].files.length; i++){
            var file = $("#formSubImage")[0].files[i];
            var text = await readFileAsDataURL(file);
            subImageFiles.push({imageNum: i, imageName: file.name, imageText:  text});

          }
          formData.append('subImageFiles', JSON.stringify(subImageFiles));
        }
        

        formData.delete('btnradio')
        formData.append('category', $('#divradio>.active').text().trim());

        var mainImageText = await readFileAsDataURL($("#formMainImage")[0].files[0])
        formData.append('mainImageText', mainImageText);
        for (var key of formData.keys()) {
          console.log(key);
        }
        for (var value of formData.values()) {
          console.log(typeof(value) + ': '+ value);
        }
        $.ajax({
          url: '/admin/newitem',
          method: 'POST',
          processData: false,
          contentType: false,
          // enctype: 'multipart/form-data',
          cache : false,
          data : formData,
          success: function(){
            alert("등록했습니다.");
            console.log('form submitted.');
            window.location.href = '/admin/item';
          },
          error: function(){
            alert("등록 실패했습니다.");
            console.log('실패');
          }
        });
      });
    </script>
  </body>
</html>
