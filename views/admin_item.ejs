</div>
<main class="container" style="overflow-y: auto">
  <h1 class="mb-5 text-center">상품관리</h1>
  <table border="0" cellspacing="5" cellpadding="5">
    <tbody><tr>
        <td>가격:</td>
        <td><input type="text" id="min" name="min"></td>
        <td>~</td>
        <td><input type="text" id="max" name="max"></td>
    </tr>
  </tbody></table>
  <div class="my-3 p-3 pb-5 bg-body rounded shadow-sm">
    <table id="table" class="table" sortable="checked">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">상품명</th>
          <th scope="col">가격</th>
          <th scope="col">재고</th>
        </tr>
      </thead>
      <tbody>
        <% for(var i=0; i<items.length; i++) {%>
          <tr>
            <td scope="row"><%= i+1 %></td>
            <td><a style="color:#212529" href="/admin/item/<%= items[i].id %>"><%= items[i].title %></a></td>
            <td><%= items[i].price %></td>
            <td>1</td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
  <a class="mt-3 mb-3 mr-3 btn btn-primary float-right" href="/admin/newitem">상품 등록</a>
</main>

    <script src="../assets/dist/js/bootstrap.bundle.min.js"></script>

    <script src="sidebars.js"></script>
    <script>
      
      $(document).ready(function(){
        
        var table = $('#table').DataTable();
        // Event listener to the two range filtering inputs to redraw on input
        console.log($.fn.dataTable.ext.search)
        $.fn.dataTable.ext.search.push(
          function( settings, data, dataIndex ) {
            var min = parseInt( $('#min').val(), 10 );
            var max = parseInt( $('#max').val(), 10 );
            var price = data[2].replace(',', '') || 0; // use data for the age column
            console.log(price)
            if ( ( isNaN( min ) && isNaN( max ) ) ||
                ( isNaN( min ) && price <= max ) ||
                ( min <= price   && isNaN( max ) ) ||
                ( min <= price   && price <= max ) )
            {
              return true;
            }
            return false;
          }
        );

        $('#min, #max').keyup( function() {
          table.draw();
        } );

        
      });
    </script>
  </body>
